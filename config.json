{
  "companies": ["https://www.linkedin.com/company/murex/posts/?feedView=all"],
  "maxEmployeesPerCompany": 11,
  "selectors": {
    "company": {
      "employeesLink": ".artdeco-card a[href^=\"/search/results/people/?currentCompany=\"]",
      "profileLink": "a[data-view-name=\"search-result-lockup-title\"][href*=\"/in/\"]",
      "nextButton": "button[data-testid^=\"pagination-controls-next-button-\"]"
    },
    "experience": {
      "experienceItem": "li[id^=profilePagedListComponent]",
      "jobTitle": "li[id^=profilePagedListComponent] .display-flex.align-items-center.mr1.hoverable-link-text.t-bold span",
      "details": ".optional-action-target-wrapper.display-flex.flex-column.full-width > .t-14.t-normal span:first-of-type",
      "description": "li[id^=profilePagedListComponent] .display-flex.flex-column.align-self-center.flex-grow-1 > div:last-of-type > ul > li ul li"
    },
    "education": {
      "educationItem": "li[id^=profilePagedListComponent]",
      "institution": "li[id^=profilePagedListComponent] .display-flex.align-items-center.mr1.hoverable-link-text.t-bold span",
      "details": ".optional-action-target-wrapper.display-flex.flex-column.full-width > .t-14.t-normal span:first-of-type"
    }
  },
  "outputFile": "linkedin_data.xlsx"
}
